{
  "name": "random-url",
  "main": "./src",
  "version": "1.0.0",
  "description": "Generate a random URL",
  "license": "MIT",
  "repository": "bendrucker/random-url",
  "author": {
    "name": "Ben Drucker",
    "email": "bvdrucker@gmail.com",
    "url": "bendrucker.me"
  },
  "engines": {
